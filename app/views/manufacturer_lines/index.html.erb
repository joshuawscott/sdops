<% content_for :subtitle do %>
  Product Lines
<% end %>
<h1>Listing manufacturer_lines</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Manufacturer</th>
    <th colspan=3> </th>
  </tr>

<% for manufacturer_line in @manufacturer_lines %>
  <tr>
    <td><%=h manufacturer_line.name %></td>
    <td><%=h manufacturer_line.manufacturer.name %></td>
    <td><%= link_to 'Show', manufacturer_line %></td>
    <td><%= link_to 'Edit', edit_manufacturer_line_path(manufacturer_line) %></td>
    <td><%= link_to 'Destroy', manufacturer_line, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New manufacturer_line', new_manufacturer_line_path %>
