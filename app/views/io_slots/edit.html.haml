- content_for :subtitle do
  I/O Slots
%h1
  Editing I/O Slot for 
  = @server.model_name

- form_for(:io_slot, :url => server_io_slot_path(@server.id, @io_slot.id), :html => { :method => :put } ) do |f|
  = render :partial => "form", :locals => { :f => f } 
%p
  = submit_tag "Update"
= link_to 'Back', server_path(@server)
