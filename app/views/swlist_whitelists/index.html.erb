<% content_for :subtitle do %>
  Servers
<% end %>
<h1>Listing swlist_whitelists</h1>

<table>
  <tr>
    <th>Pattern</th>
    <th colspan='3'>Options</th>
  </tr>

<% for swlist_whitelist in @swlist_whitelists %>
  <tr>
    <td><%=h swlist_whitelist.pattern %></td>
    <td><%= link_to 'Show', swlist_whitelist %></td>
    <td><%= link_to 'Edit', edit_swlist_whitelist_path(swlist_whitelist) %></td>
    <td><%= link_to 'Destroy', swlist_whitelist, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New swlist_whitelist', new_swlist_whitelist_path %>
