<h1>Editing swlist_whitelist</h1>

<% form_for(@swlist_whitelist) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :pattern %><br />
    <%= f.text_field :pattern %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @swlist_whitelist %> |
<%= link_to 'Back', swlist_whitelists_path %>
