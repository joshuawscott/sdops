-content_for :subtitle do
  Auditing
#section1
  .boxed
    %h2 Audits by type&nbsp;
    %ul
      - @models.each do |m|
        %li{:style=>'font-weight:bold;padding-bottom:1px;list-style:none'}
          =link_to m, url_for(:action => 'focus', :params => {:model => m })
  .boxed
    %h2 Audits by User&nbsp;
    %ul
      - @users.each do |u|
        %li{:style=>'font-weight:bold;padding-bottom:1px;list-style:none'}
          =link_to u, url_for(:action => 'user', :params => {:user => u })
