-content_for :subtitle do
  Edit SW Support Price
#section1
  =error_messages_for :sw_support_price
  #sw_price.boxed
    %h2 Edit SW Support Price
    - form_for(:sw_support_price, :url => sw_support_price_path(@sw_support_price), :html => { :method => :put }) do |f|
      =render :partial => "form", :locals => {:f => f}
      %p
        %label{:for => "sw_support_price_modified_at"}
          Effective Date
        = f.text_field :modified_at
      %p
        = f.submit "Update"
      %p= link_to "Cancel", sw_support_prices_path

